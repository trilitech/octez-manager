{
  "id": null,
  "uid": "octez-ui",
  "title": "Octez Manager UI",
  "schemaVersion": 36,
  "version": 1,
  "tags": ["octez", "metrics"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {"from": "now-30m", "to": "now"},
  "timepicker": {},
  "templating": {"list": []},
  "annotations": {"list": []},
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Render p90 (ms)",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "refId": "A",
          "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.90\"}",
          "legendFormat": "{{page}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "stat",
      "title": "Key→Render p90 (ms)",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "refId": "A",
          "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.90\"}",
          "legendFormat": "p90"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "stat",
      "title": "BG Queue Depth",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
