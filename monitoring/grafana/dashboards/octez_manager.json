{
  "id": null,
  "uid": "octez-ui",
  "title": "Octez Manager UI",
  "schemaVersion": 36,
  "version": 1,
  "tags": ["octez", "metrics"],
  "timezone": "browser",
  "refresh": "5s",
  "time": {"from": "now-30m", "to": "now"},
  "timepicker": {},
  "templating": {"list": []},
  "annotations": {"list": []},
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Service Status",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_service_status", "legendFormat": "{{service}} - {{state}}"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "value": "1", "text": "Active"},
            {"type": "value", "value": "0", "text": "Inactive"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Service Status Over Time",
      "gridPos": {"h": 6, "w": 16, "x": 8, "y": 0},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_service_status", "legendFormat": "{{service}}"}
      ],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}, "tooltip": {"mode": "multi"}},
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "type": "stat",
      "title": "Render p90 (ms)",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "refId": "A",
          "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.90\"}",
          "legendFormat": "{{page}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "stat",
      "title": "Key→Render p90 (ms)",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "refId": "A",
          "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.90\"}",
          "legendFormat": "p90"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "stat",
      "title": "BG Queue Depth",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 6},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_bg_queue_depth", "legendFormat": "depth"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "timeseries",
      "title": "Render p50/p90/p99 (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.50\"}", "legendFormat": "p50 {{page}}"},
        {"refId": "B", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.90\"}", "legendFormat": "p90 {{page}}"},
        {"refId": "C", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.99\"}", "legendFormat": "p99 {{page}}"}
      ],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}, "tooltip": {"mode": "single"}}
    },
    {
      "type": "timeseries",
      "title": "Key→Render Latency (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.50\"}", "legendFormat": "p50"},
        {"refId": "B", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.90\"}", "legendFormat": "p90"},
        {"refId": "C", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.99\"}", "legendFormat": "p99"}
      ],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}, "tooltip": {"mode": "single"}}
    },
    {
      "type": "timeseries",
      "title": "Background Wait (ms)",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_bg_wait_ms{quantile=\"0.90\"}", "legendFormat": "p90"},
        {"refId": "B", "expr": "octez_manager_bg_wait_ms{quantile=\"0.99\"}", "legendFormat": "p99"}
      ],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}, "tooltip": {"mode": "single"}}
    },
    {
      "type": "timeseries",
      "title": "Render Count",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_ui_render_duration_ms_count", "legendFormat": "count {{page}}"}
      ],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}, "tooltip": {"mode": "single"}}
    }
  ]
}
