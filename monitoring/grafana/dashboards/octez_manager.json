    {
      "type": "timeseries",
      "title": "Key→Render Latency (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.50\"}", "legendFormat": "p50"},
        {"refId": "B", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.90\"}", "legendFormat": "p90"},
        {"refId": "C", "expr": "octez_manager_ui_key_to_render_ms{quantile=\"0.99\"}", "legendFormat": "p99"}
