        {"refId": "A", "expr": "octez_manager_bg_queue_depth", "legendFormat": "depth"}
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none"
      }
    },
    {
      "type": "timeseries",
      "title": "Render p50/p90/p99 (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {"refId": "A", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.50\"}", "legendFormat": "p50 {{page}}"},
        {"refId": "B", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.90\"}", "legendFormat": "p90 {{page}}"},
        {"refId": "C", "expr": "octez_manager_ui_render_duration_ms{quantile=\"0.99\"}", "legendFormat": "p99 {{page}}"}
